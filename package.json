{
  "homepage": "http://khoathanglong.github.io/leaderboard",
  "name": "my-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "gh-pages": "^1.0.0",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-git-install": "^0.3.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
